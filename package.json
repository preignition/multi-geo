{
  "description": "lit-element building blocks elements for building geo visualization (based on d3.js v5).",
  "keywords": [
    "web-component",
    "web-components",
    "d3.js",
    "maps",
    "geo",
    "charts",
    "lit-element",
    "polymer",
    "analytics",
    "Polymer",
    "dataviz"
  ],
  "repository": "https://github.com/preignition/multi-geo",
  "homepage": "webcomponents.org/preignition/multi-geo",
  "name": "@preignition/multi-geo",
  "version": "4.2.1",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "dependencies": {
    "@polymer/polymer": "^3.0.0",
    "@preignition/multi-chart": "file:../multi-chart",
    "@preignition/preignition-mixin": "^1",
    "d3-array": "^2.7.1",
    "d3-color": "^2.0.0",
    "d3-dispatch": "^2.0.0",
    "d3-ease": "^2.0.0",
    "d3-format": "^2.0.0",
    "d3-geo": "^2.0.1",
    "d3-hexbin": "^0.2.2",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.2.2",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-selection-multi": "^1.0.1",
    "d3-time": "^2.0.0",
    "d3-timer": "^2.0.0",
    "d3-transition": "^2.0.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "topojson-client": "^3.1.0",
    "topojson-simplify": "^3.0.3"
  },
  "files": [
    "src/**",
    "elements/**",
    "index.js",
    "index.elements.js",
    "index.class.js"
  ],
  "devDependencies": {
    "@open-wc/testing": "~0.11.1",
    "@open-wc/testing-karma": "^3.0.0",
    "@preignition/preignition-demo": "link:../preignition-demo",
    "@preignition/preignition-widget": "^0.1.0",
    "@vaadin/router": "^1.6.0",
    "@vaadin/vaadin-tabs": "3.2.0",
    "@webcomponents/webcomponentsjs": "^1.3.3",
    "api-viewer-element": "^0.3.13",
    "d3-svg-legend": "github:christophe-g/d3-legend#promise",
    "es-dev-server": "^1.40.0",
    "marked": "^1.1.1",
    "pwa-helpers": "^0.9.1",
    "snowpack": "^2.0.2"
  },
  "snowpack": {
    "install": [
      "@morbidick/lit-element-notify",
      "@polymer/polymer/lib/utils/mixin.js",
      "@polymer/polymer/lib/utils/case-map.js",
      "@polymer/polymer/lib/utils/async.js",
      "@polymer/polymer/lib/utils/debounce.js",
      "@preignition/preignition-mixin",
      "@preignition/multi-chart",
      "@preignition/preignition-widget",
      "@preignition/preignition-demo",
      "@vaadin/router",
      "@vaadin/vaadin-tabs",
      "pwa-helpers/media-query.js",
      "api-viewer-element",
      "tslib",
      "marked",
      "d3-array",
      "d3-geo",
      "d3-hexbin",
      "d3-interpolate",
      "d3-selection",
      "d3-selection-multi",
      "d3-selection-multi/src/selection/attrs.js",
      "d3-selection-multi/src/transition/attrs.js",
      "d3-transition",
      "d3-scale",
      "d3-time",
      "d3-scale-chromatic",
      "d3-svg-legend",
      "lit-element",
      "topojson-client",
      "topojson-simplify",
      "lit-html/directives/cache.js",
      "lit-html/directives/unsafe-html.js",
      "lit-html/directives/until.js",
      "lit-html/directives/style-map.js",
      "lit-html"
    ],
    "installOptions": {
      "rollup": {
        "dedupe": [
          "lit-html",
          "lit-element",
          "marked",
          "topojson-client",
          "topojson-simplify",
          "api-viewer-element",
          "d3-selection",
          "d3-transition"
        ]
      }
    }
  },
  "scripts": {
    "start": "es-dev-server -v",
    "generate-docs-json": "wca analyze src/**/*.js  --outFiles docs/{tagname}.json ",
    "generate-docs-md": "wca analyze src/**/*.js  --outFiles docs/{tagname}.md ",
    "generate-docs:helper": "wca analyze src/helper/** --outFile docs/helper.json",
    "generate-docs:wrapper": "wca analyze src/d3-wrapper/** --outFile docs/wrapper.json",
    "generate-docs:drawable": "wca analyze src/drawable/** --outFile docs/drawable.json",
    "generate-docs:chart": "wca analyze src/chart/** --outFile docs/chart.json",
    "generate-docs:container": "wca analyze src/container/** --outFile docs/container.json",
    "generate-docs": "npm run generate-docs-md && npm run generate-docs-json && npm run generate-docs:helper && npm run generate-docs:wrapper && npm run generate-docs:container && npm run generate-docs:chart && npm run generate-docs:drawable",
    "prepublishOnly": "npm run generate-docs && pnpx snowpack i",
    "postpublish": "git push && git push --tags",
    "test": "karma start --coverage",
    "test:compatibility": "karma start --compatibility all --coverage"
  },
  "author": {
    "name": "Christophe Geiser",
    "email": "christophe@preignition.org"
  }
}
